<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet main.css?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" id="BorderPane" fx:id="consolePane" maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="419.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="uk.org.il2ssd.jfx.ConsolePresenter">
    <bottom>
        <AnchorPane minHeight="-1.0" prefHeight="-1.0" prefWidth="500.0">
            <children>
                <TextField fx:id="commandEntryField" disable="true" maxHeight="30.0" minHeight="30.0" prefHeight="30.0"
                           prefWidth="-1.0" promptText="&lt;enter command&gt;" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="5.0"/>
            </padding>
        </AnchorPane>
    </bottom>
    <center>
        <AnchorPane prefHeight="374.0" prefWidth="500.0">
            <TextArea fx:id="consoleTextArea" disable="false" editable="false" focusTraversable="false"
                      prefHeight="-1.0" prefWidth="-1.0" promptText="" text="&lt;disconnected&gt;" wrapText="false"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0"/>
            <padding>
                <Insets bottom="5.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </AnchorPane>
    </center>
</BorderPane>
